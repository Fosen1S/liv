local key = _G.Key
local check = "https://sharinganx123.000webhostapp.com/check.php?key=" .. key --Key Checking 
-- local check = "https://sharinganx123.000webhostapp.com/modified%20main.lua?key="

if game:HttpGet(check) == "Whitelisted" then
loadstring(game:HttpGet"https://sharinganx123.000webhostapp.com/paidcheck.php")()
wait()
loadstring(game:HttpGet("https://sharinganx123.000webhostapp.com/placeID.lua"))() --PlaceID
else
    game.Players.LocalPlayer:Kick("Try Again!")  
end

